{
  "DataExporter": {
    "ElasticsearchUrl": "http://localhost:9200",
    "IndexPattern": "bond-calculations-*",
    "EventIdFilter": 9001,
    "MaxRecordsToFetch": 1000,
    "OutputPath": "../tests/BondCalculationService.Tests/GoldenDataset/production-cases.json"
  },
  "Curation": {
    "MaxTestCases": 50,
    "RequireSuccess": true,
    "MinDaysOld": 1,
    "MaxDaysOld": 90
  },
  "Anonymization": {
    "AnonymizeCusip": true,
    "AnonymizeSettlementDate": false,
    "CusipPrefix": "ANON"
  }
}
