{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Grpc": "Debug",
      "GrpcService": "Debug"
    }
  },
  "Greeter": {
    "GreetingPrefix": "Hello from Development",
    "AppVersion": "1.0.0-dev",
    "IncludeMetadata": true,
    "MaxNameLength": 100
  },
  "ExternalService": {
    "BaseUrl": "https://dev-api.example.com",
    "ApiKey": "dev-api-key-12345",
    "TimeoutSeconds": 60,
    "EnableRetry": true,
    "MaxRetryAttempts": 5
  }
}
